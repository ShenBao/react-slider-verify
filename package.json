{
  "name": "react-slider-verify",
  "version": "0.0.1",
  "description": "react-slider-verify",
  "main": "index.js",
  "scripts": {
    "example": "webpack serve --config webpack.demo.config.js",
    "example:build": "webpack --config webpack.demo.config.js",
    "rm": "rimraf ./es && rimraf ./lib",
    "watch:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es --watch --extensions '.ts,.tsx'",
    "watch:lib": "cross-env NODE_ENV=production BABEL_ENV=lib babel src --out-dir lib --watch --extensions '.ts,.tsx'",
    "watch": "concurrently \"npm run watch:es\" \"npm run watch:lib \"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es --extensions '.ts,.tsx'",
    "build:lib": "cross-env NODE_ENV=production BABEL_ENV=lib babel src --out-dir lib --extensions '.ts,.tsx'",
    "build": "npm run rm&& npm run build:es && npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShenBao/react-slider-verify.git"
  },
  "keywords": [
    "react-slider-verify"
  ],
  "author": "ShenBao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShenBao/react-slider-verify/issues"
  },
  "homepage": "https://github.com/ShenBao/react-slider-verify#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@types/node": "^16.3.3",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "concurrently": "^6.2.0",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.1.0",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^5.3.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
